    <string>bash -c "/opt/gls/clarity/bin/java -jar /opt/gls/clarity/extensions/ngs-common/v5/EPP/ngs-extensions.jar -i {processURI:v2:http} -u {username} -p {password} script:assignQC -log {compoundOutputFileLuid1} -qcResult {compoundOutputFileLuid2}"</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <process-input>
