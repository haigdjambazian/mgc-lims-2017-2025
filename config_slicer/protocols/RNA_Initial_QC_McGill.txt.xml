<ptp:process-type xmlns:ptp="http://genologics.com/ri/processtype" name="Aggregate QC (RNA) McGill 1.0">
  <field-definition name="Bioanalyzer QC (RNA) McGill 1.0"/>
  <field-definition name="CaliperGX QC (RNA) McGill 1.0"/>
  <field-definition name="Comments"/>
  <field-definition name="Copy task 1 - Source Field"/>
  <field-definition name="Copy task 1 - Source Step"/>
  <field-definition name="Copy task 2 - Source Field"/>
  <field-definition name="Copy task 2 - Source Step"/>
  <field-definition name="Copy task 3 - Source Field"/>
  <field-definition name="Copy task 3 - Source Step"/>
  <field-definition name="Femto Pulse QC (RNA) McGill 1.0"/>
  <field-definition name="Qubit QC (RNA) McGill 1.0"/>
  <field-definition name="Tapestation QC (RNA) McGill 1.0"/>
  <parameter name="Aggregate QC Flags and Copy Fields">
    <string>bash -c "/opt/gls/clarity/bin/java -jar /opt/gls/clarity/extensions/ngs-common/v5/EPP/ngs-extensions.jar -i {processURI:v2:http} -u {username} -p {password} script:setUDF -f 'Progress' -t '//input/@uri-&gt;//sample/@uri' -v 'Initial sample QC complete' script:aggregateQC -c 'true' -a 'true' -log {compoundOutputFileLuid0} -aggregatelog {compoundOutputFileLuid1} -copylog {compoundOutputFileLuid2}"</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <process-input>
    <artifact-type>Analyte</artifact-type>
    <display-name>Analyte</display-name>
    <remove-working-flag>false</remove-working-flag>
  </process-input>
  <process-output>
    <artifact-type>ResultFile</artifact-type>
    <display-name>ResultFile</display-name>
    <output-generation-type>PerAllInputs</output-generation-type>
    <variability-type>Fixed</variability-type>
    <number-of-outputs>3</number-of-outputs>
    <output-name>{LIST:QC UDF Copy Log,Aggregation Script Log,QC Flag Aggregation Log}</output-name>
    <field-definition name="RIN"/>
  </process-output>
  <process-type-attribute name="ConsumeVolume">false</process-type-attribute>
  <process-type-attribute name="ContextCode">AGR</process-type-attribute>
  <process-type-attribute name="DefaultPlacementPattern">Place samples vertically</process-type-attribute>
  <process-type-attribute name="Enabled">true</process-type-attribute>
  <process-type-attribute name="Family">Configured</process-type-attribute>
  <process-type-attribute name="IconConstant">HYBRIDIZED_SAMPLE_ICON</process-type-attribute>
  <process-type-attribute name="InheritsQC">false</process-type-attribute>
  <process-type-attribute name="ModifyInputOutput">All</process-type-attribute>
  <process-type-attribute name="OnlyEnableDoneAtLastPanel">false</process-type-attribute>
  <process-type-attribute name="OpenPostProcess">false</process-type-attribute>
  <process-type-attribute name="OutputContextCode">AG</process-type-attribute>
  <process-type-attribute name="OutputSorting">Input LIMS ID</process-type-attribute>
  <process-type-attribute name="ProcessGroup">1. RNA Initial QC</process-type-attribute>
  <process-type-attribute name="ProcessTabView">Display and maximize the Input/Output Explorer mini-tabbed page</process-type-attribute>
  <process-type-attribute name="QCAdjustmentType">None</process-type-attribute>
  <process-type-attribute name="ShowInButtonBar">false</process-type-attribute>
  <process-type-attribute name="ShowInExplorer">false</process-type-attribute>
  <process-type-attribute name="SkipInputPanel">true</process-type-attribute>
  <process-type-attribute name="SupportsExternalProgram">true</process-type-attribute>
  <process-type-attribute name="VolumeAdjustmentType">None</process-type-attribute>
</ptp:process-type>
<ptp:process-type xmlns:ptp="http://genologics.com/ri/processtype" name="Qubit QC (RNA) McGill 1.0">
  <field-definition name="Comments"/>
  <field-definition name="Criteria 1 - Operator"/>
  <field-definition name="Criteria 1 - Source Data Field"/>
  <field-definition name="Criteria 1 - Threshold Value"/>
  <field-definition name="Criteria 2 - Operator"/>
  <field-definition name="Criteria 2 - Source Data Field"/>
  <field-definition name="Criteria 2 - Threshold Value"/>
