	<ProcessTypes>
<ptp:process-type xmlns:ptp="http://genologics.com/ri/processtype" name="DNA Samples QC">
  <field-definition name="Concentration Threshold (ng/uL)"/>
  <field-definition name="Criteria 1 - Operator"/>
  <field-definition name="Criteria 1 - Source Data Field"/>
  <field-definition name="Criteria 1 - Threshold Value"/>
  <field-definition name="Criteria 2 - Operator"/>
  <field-definition name="Criteria 2 - Source Data Field"/>
  <field-definition name="Criteria 2 - Threshold Value"/>
  <parameter name="0.Print barcodes">
    <string>bash -c "/usr/bin/python3.5  /opt/gls/clarity/customextensions/SPARK/create_plate_barcode_csv.py  -stepURI_v2={stepURI:v2}   -user_psw={username}:{password}  -labelSTD='1'    &gt; /opt/gls/clarity/ai/temp/barcodes_tmp.csv  &amp;&amp;   /usr/bin/python3.5 /opt/gls/clarity/customextensions/print_zebra_test_3_am.py  /opt/gls/clarity/ai/temp/barcodes_tmp.csv &gt;  /opt/gls/clarity/ai/temp/barcodes_print_output.txt  " 
</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="2.Apply quantified values">
    <string>bash -l -c  "/usr/bin/python3.5 /opt/gls/clarity/customextensions/SPARK/spark_apply_quant.py  -fileLuid={compoundOutputFileLuid0}  -stepURI_v2={stepURI:v2}   -processURI_v2={processURI:v2}  -user_psw={username}:{password}  &gt; {compoundOutputFileLuid21}.txt"</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="2.Assign Concentration from Submitted Samples">
    <string>bash -l -c "/opt/gls/clarity/bin/java -jar /opt/gls/clarity/extensions/ngs-common/v5/EPP/ngs-extensions.jar -i {stepURI:v2:http} -u {username} -p {password} script:evaluateDynamicExpression -t false -h false -exp 'if (submittedSample.hasValue( ::Sample Conc.:: )) {output.::Concentration:: = submittedSample.::Sample Conc.::};  if (submittedSample.hasValue( ::Sample Conc Units:: )) {output.::Conc. Units::=submittedSample.::Sample Conc Units::} ; if (input.hasValue( ::Concentration:: )) {output.::Concentration:: = input.::Concentration::};  ' -log {compoundOutputFileLuid0}"
</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="3.Assign QC flags">
    <string>bash -l -c "/opt/gls/clarity/bin/java -jar /opt/gls/clarity/extensions/ngs-common/v5/EPP/ngs-extensions.jar -i {stepURI:v2:http} -u {username} -p {password} script:evaluateDynamicExpression -t false -h false -exp 'if (output.::Concentration:: &gt;= step.::Concentration Threshold (ng/uL):: ) {output.QC = true} else {output.QC = false}' -log {compoundOutputFileLuid2}" </string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="4. Tubes AutoPlacement">
    <string>bash -l -c " /usr/bin/python  /opt/gls/clarity/customextensions/Common/autoplacement_mix_tubes_plates.py -u {username} -p {password} -o 'Analyte' -s {stepURI:v2} &gt; {compoundOutputFileLuid0}_auto-log.txt"

</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <process-input>
    <artifact-type>Analyte</artifact-type>
    <display-name>Analyte</display-name>
    <remove-working-flag>false</remove-working-flag>
  </process-input>
  <process-output>
    <artifact-type>Analyte</artifact-type>
    <display-name>Analyte</display-name>
    <output-generation-type>PerInput</output-generation-type>
    <variability-type>Fixed</variability-type>
    <number-of-outputs>1</number-of-outputs>
    <output-name>{InputItemName}</output-name>
    <assign-working-flag>false</assign-working-flag>
  </process-output>
  <process-output>
    <artifact-type>ResultFile</artifact-type>
    <display-name>SharedResultFile</display-name>
    <output-generation-type>PerAllInputs</output-generation-type>
    <variability-type>Fixed</variability-type>
    <number-of-outputs>3</number-of-outputs>
    <output-name>{LIST:Log file,Assign Report,Assign Log}</output-name>
    <field-definition name="File"/>
  </process-output>
  <process-type-attribute name="ConsumeVolume">false</process-type-attribute>
  <process-type-attribute name="ContextCode">PGD</process-type-attribute>
  <process-type-attribute name="DefaultPlacementPattern">Place samples vertically</process-type-attribute>
  <process-type-attribute name="Enabled">true</process-type-attribute>
  <process-type-attribute name="Family">Configured</process-type-attribute>
