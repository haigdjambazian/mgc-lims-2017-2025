<ptp:process-type xmlns:ptp="http://genologics.com/ri/processtype" name="Sample Normalization Generic">
  <field-definition name="9th plate offset"/>
  <field-definition name="Concentration from"/>
  <field-definition name="Controls"/>
  <field-definition name="Destination Concentration (ng/ul)"/>
  <field-definition name="Destination Volume (ul)"/>
  <field-definition name="Quantity (ng)"/>
  <field-definition name="Target"/>
  <field-definition name="Threshold, ng/ul"/>
  <parameter name="0. Print barcodes">
    <string>bash -c "/usr/bin/python3.5  /opt/gls/clarity/customextensions/Common/create_barcodes_csv.py   -stepURI_v2={stepURI:v2}   -user={username} -psw={password}  -containerSource=io &gt; /opt/gls/clarity/ai/temp/barcodes_tmp.csv  &amp;&amp;   /usr/bin/python3.5 /opt/gls/clarity/customextensions/print_zebra_test_3_am.py  /opt/gls/clarity/ai/temp/barcodes_tmp.csv &gt;  /opt/gls/clarity/ai/temp/barcodes_print_output.txt  " 
</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="1. Normalize samples">
    <string>bash -c -l "/usr/bin/python3.5  /opt/gls/clarity/customextensions/Common/concentration_normalization_excel_srt.py   -stepURI_v2={stepURI:v2}  -processLuid={processLuid} -user_psw={username}:{password}  -ar=Analyte  -ComponentLuids='{compoundOutputFileLuids}'   &gt; {compoundOutputFileLuid0}_for_robot.csv"</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="2. Route samples">
    <string>bash -l -c "/opt/gls/clarity/bin/java -jar /opt/gls/clarity/extensions/ngs-common/v5/EPP/ngs-extensions.jar -i {stepURI:v2:http} -u {username} -p {password} script:evaluateDynamicExpression -t true -h false -exp 'if (output.::Go To Next Step:: == true) { nextStep = ::REPEAT:: } else { nextStep = :: DNA Amplification McGill 1.1:: }' -log {compoundOutputFileLuid2}"</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="3. Route by python">
    <string>bash -c "/usr/bin/python /opt/gls/clarity/customextensions/Common/route_by_template.py -u {username} -p {password} -s {stepURI:v2} -r 'Go To Next Step,False,Affymetrix Axiom Genotyping McGill 1.0,DNA Amplification McGill 1.1\nGo To Next Step,True,Sample Normalization Generic,Sample Normalization Extra Plate' -l {compoundOutputFileLuid2}"</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="AutoPlacement">
    <string>/usr/bin/python /opt/gls/clarity/customextensions/Common/autoplaceSamplesDefaultMulti.py -l {processLuid} -u {username} -p {password} -s {stepURI:v2}</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="Control Placement with LeftOver">
    <string>bash -c -l "/usr/bin/python /opt/gls/clarity/customextensions/Common/autoplace_controls_leftover_sorted.py  -u {username} -p {password} -s {stepURI:v2} --placementtemplate /opt/gls/clarity/ai/temp/{processLuid}_norm_template.csv --containertype '96 well plate'  &gt; /opt/gls/clarity/ai/temp/{processLuid}_autoplacement_log.txt"</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <process-input>
    <artifact-type>Analyte</artifact-type>
    <display-name>Analyte</display-name>
    <remove-working-flag>false</remove-working-flag>
  </process-input>
  <process-output>
    <artifact-type>Analyte</artifact-type>
    <display-name>Analyte</display-name>
    <output-generation-type>PerInput</output-generation-type>
    <variability-type>VariableByInput</variability-type>
    <output-name>{InputItemName}</output-name>
    <field-definition name="Pooling Group"/>
    <field-definition name="Go To Next Step"/>
    <assign-working-flag>true</assign-working-flag>
  </process-output>
  <process-output>
    <artifact-type>ResultFile</artifact-type>
    <display-name>ResultFile</display-name>
    <output-generation-type>PerAllInputs</output-generation-type>
    <variability-type>Fixed</variability-type>
    <number-of-outputs>3</number-of-outputs>
    <output-name>{LIST: CSV file for robot,Normalization Log file,Mapping file }</output-name>
  </process-output>
