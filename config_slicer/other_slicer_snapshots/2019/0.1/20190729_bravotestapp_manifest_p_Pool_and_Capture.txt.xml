	<ProcessTypes>
<ptp:process-type xmlns:ptp="http://genologics.com/ri/processtype" name="Capture">
  <field-definition name="Capture Bait Version"/>
  <field-definition name="Capture Date"/>
  <field-definition name="Capture Kit Type"/>
  <field-definition name="Capture Technician"/>
  <field-definition name="Lot Ampure Beads"/>
  <field-definition name="PCR Cycles"/>
  <field-definition name="PCR Enzyme Lot"/>
  <field-definition name="PCR Machine Name"/>
  <field-definition name="Pipetting Method"/>
  <field-definition name="Reference"/>
  <field-definition name="Wash Reagent Lot"/>
  <parameter name="1.Library Volume in Pool">
    <string>bash -l -c "/opt/gls/clarity/bin/java -jar /opt/gls/clarity/extensions/ngs-common/v5/EPP/ngs-extensions.jar -i {stepURI:v2:http} -u {username} -p {password} script:evaluateDynamicExpression -t false -h false -exp 'if (!output.hasValue( ::Pool Volume (ul):: )) {output.::Pool Volume (ul):: = 0}; if (output.hasValue( ::Pool Volume (ul):: )) {output.::Pool Volume (ul):: = output.::Pool Volume (ul):: + input.::Library Volume To Use (ul)::}; if (!output.hasValue( ::Number of Sample in Pool:: )) {output.::Number of Sample in Pool:: = 0}; if (output.hasValue( ::Number of Sample in Pool:: )) {output.::Number of Sample in Pool:: = output.::Number of Sample in Pool:: + 1};' -log {compoundOutputFileLuid0}"</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="Auto Pooling">
    <string>bash -c -l "/usr/bin/python /opt/gls/clarity/customextensions/Common/autopool_2018.py  -u {username} -p {password}  -s {stepURI:v2} "</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <process-input>
    <artifact-type>Analyte</artifact-type>
    <display-name>Sample</display-name>
    <remove-working-flag>false</remove-working-flag>
  </process-input>
  <process-output>
    <artifact-type>Analyte</artifact-type>
    <display-name>Sample</display-name>
    <output-generation-type>PerAllInputs</output-generation-type>
    <variability-type>Fixed</variability-type>
    <number-of-outputs>1</number-of-outputs>
    <output-name>{PoolName}</output-name>
    <assign-working-flag>true</assign-working-flag>
  </process-output>
  <process-output>
    <artifact-type>ResultFile</artifact-type>
    <display-name>ResultFile</display-name>
    <output-generation-type>PerAllInputs</output-generation-type>
    <variability-type>Fixed</variability-type>
    <number-of-outputs>1</number-of-outputs>
    <output-name>{LIST:Log file}</output-name>
  </process-output>
  <process-type-attribute name="ConsumeVolume">true</process-type-attribute>
  <process-type-attribute name="ContextCode">PSA</process-type-attribute>
  <process-type-attribute name="DefaultPlacementPattern">Place samples horizontally</process-type-attribute>
  <process-type-attribute name="DefaultSort">ROW</process-type-attribute>
  <process-type-attribute name="Enabled">true</process-type-attribute>
  <process-type-attribute name="Family">PoolSamples</process-type-attribute>
  <process-type-attribute name="IconConstant">POOLING_RUN_MULTI_ICON</process-type-attribute>
  <process-type-attribute name="ModifyInputOutput">None</process-type-attribute>
  <process-type-attribute name="NamingTagHandlers">com.genologics.platform.behaviourmanagement.process.commands.outputnaming.PoolNamingTagHandler</process-type-attribute>
  <process-type-attribute name="OnlyEnableDoneAtLastPanel">false</process-type-attribute>
  <process-type-attribute name="OpenPostProcess">false</process-type-attribute>
  <process-type-attribute name="OutputContextCode">PS</process-type-attribute>
  <process-type-attribute name="OutputSorting">Input LIMS ID</process-type-attribute>
  <process-type-attribute name="ProcessGroup">Transfer Processes</process-type-attribute>
  <process-type-attribute name="ProcessTabView">Use the default display</process-type-attribute>
  <process-type-attribute name="QCAdjustmentType">None</process-type-attribute>
  <process-type-attribute name="SameReagent">true</process-type-attribute>
  <process-type-attribute name="ShowInButtonBar">false</process-type-attribute>
  <process-type-attribute name="ShowInExplorer">false</process-type-attribute>
  <process-type-attribute name="SkipInputPanel">false</process-type-attribute>
  <process-type-attribute name="SupportsExternalProgram">true</process-type-attribute>
  <process-type-attribute name="VolumeAdjustmentType">None</process-type-attribute>
  <process-type-attribute name="WorkspaceColumns">8</process-type-attribute>
  <process-type-attribute name="WorkspaceRows">4</process-type-attribute>
</ptp:process-type>
<ptp:process-type xmlns:ptp="http://genologics.com/ri/processtype" name="Library Normalization for Pooling">
  <field-definition name="Concentration from"/>
  <field-definition name="Destination Concentration (ng/ul)"/>
  <field-definition name="Destination Volume (ul)"/>
  <field-definition name="Pooling Status"/>
  <field-definition name="Target"/>
  <field-definition name="Total Quantity for Pool (ng)"/>
  <parameter name="0. Print barcodes">
    <string>bash -c "/usr/bin/python3.5  /opt/gls/clarity/customextensions/Common/create_barcodes_csv.py   -stepURI_v2={stepURI:v2}   -user={username} -psw={password}  &gt; /opt/gls/clarity/ai/temp/barcodes_tmp.csv  &amp;&amp;   /usr/bin/python3.5 /opt/gls/clarity/customextensions/print_zebra_test_3_am.py  /opt/gls/clarity/ai/temp/barcodes_tmp.csv &gt;  /opt/gls/clarity/ai/temp/barcodes_print_output.txt  " 
</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="1.Index Validation">
    <string>bash -l -c "/usr/bin/python3.5 /opt/gls/clarity/customextensions/Common/create_csv_4_library_index_validation.py   -stepURI_v2={stepURI:v2}   -user={username} -psw={password}  -ar=Analyte  -ComponentLuids='{compoundOutputFileLuids}'  &gt;  /opt/gls/clarity/ai/temp/{processLuid}_index_val.csv &amp;&amp; sh /opt/gls/clarity/customextensions/Common/hamming_dist_v4.sh  /opt/gls/clarity/ai/temp/{processLuid}_index_val.csv  /opt/gls/clarity/customextensions/Common &amp;&amp; /usr/bin/python /opt/gls/clarity/customextensions/Common/upload_custom_reports.py   -s {stepURI:v2}   -u {username} -p {password}   -t  /opt/gls/clarity/ai/temp/{processLuid}_index_val.udfs.txt  &amp;&amp; mv /opt/gls/clarity/ai/temp/{processLuid}_index_val.html  {compoundOutputFileLuid5}_{processLuid}_index_val.html "</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="2. Normalize samples">
    <string>bash -c -l "/usr/bin/python3.5 /opt/gls/clarity/customextensions/Common/concentration_lib_normalization_sorted_robot.py   -stepURI_v2={stepURI:v2}   -user_psw={username}:{password}  -ar=Analyte  -ComponentLuids='{compoundOutputFileLuids}'   -reports='0' "</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="3. QC Validation">
    <string>bash -l -c "/opt/gls/clarity/bin/java -jar /opt/gls/clarity/extensions/ngs-common/v5/EPP/ngs-extensions.jar -i {stepURI:v2:http} -u {username} -p {password} script:evaluateDynamicExpression -t false -h false -excludeControls true -exp 'if (output.::Volume QC:: == ::Pass:: &amp;&amp; output.::Pooling QC:: == ::Pass:: ) {output.QC=true} else {output.QC=false} ' -log {compoundOutputFileLuid4} &amp;&amp; /usr/bin/python3.5 /opt/gls/clarity/customextensions/Common/concentration_lib_normalization_sorted_robot.py   -stepURI_v2={stepURI:v2}   -user_psw={username}:{password}  -ar=Analyte  -ComponentLuids='{compoundOutputFileLuids}'   -reports='1' -qc='1' &gt; {compoundOutputFileLuid0}_for_robot.csv "
</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="Autoplacement  Generic Artifacts">
    <string>bash -l -c " /usr/bin/python /opt/gls/clarity/customextensions/Common/autoplacement_generic_artifact.py -u {username} -p {password}  -s {stepURI:v2}  -t /opt/gls/clarity/ai/temp/{processLuid}_autoplacement_generic_artifacts_template.csv  --suffix '_{processLuid}'  "</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="Library Volume Adjustment">
    <string>bash -l -c "/opt/gls/clarity/bin/java -jar /opt/gls/clarity/extensions/ngs-common/v5/EPP/ngs-extensions.jar -i {stepURI:v2:http} -u {username} -p {password} script:evaluateDynamicExpression -t false -h false -excludeControls true -exp 'if (output.QC == true) {input.::Library Volume (ul):: =  input.::Library Volume (ul)::-output.::Library Volume To Use (ul)::} ' -log {compoundOutputFileLuid4}"
</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <parameter name="Transfering LibQC data">
    <string>bash -l -c "/opt/gls/clarity/bin/java -jar /opt/gls/clarity/extensions/ngs-common/v5/EPP/ngs-extensions.jar -i {stepURI:v2:http} -u {username} -p {password} script:evaluateDynamicExpression -t false -h false -exp 'output.::Concentration:: = input.::Concentration::; output.::Conc. Units:: = input.::Conc. Units::; output.::Library Volume (ul):: = input.::Library Volume (ul)::; output.::Size (bp):: = input.::Size (bp):: ; ' -log {compoundOutputFileLuid1}"</string>
    <run-program-per-event>false</run-program-per-event>
    <channel>limsserver</channel>
    <invocation-type>PostProcess</invocation-type>
  </parameter>
  <process-input>
    <artifact-type>Analyte</artifact-type>
    <display-name>Analyte</display-name>
    <remove-working-flag>false</remove-working-flag>
  </process-input>
  <process-output>
    <artifact-type>Analyte</artifact-type>
    <display-name>Analyte</display-name>
    <output-generation-type>PerInput</output-generation-type>
    <variability-type>Fixed</variability-type>
    <number-of-outputs>1</number-of-outputs>
    <output-name>{InputItemName}</output-name>
    <field-definition name="Concentration"/>
    <field-definition name="Conc. Units"/>
    <field-definition name="Pooling Group"/>
    <field-definition name="Go To Next Step"/>
    <assign-working-flag>true</assign-working-flag>
  </process-output>
  <process-output>
    <artifact-type>ResultFile</artifact-type>
    <display-name>ResultFile</display-name>
    <output-generation-type>PerAllInputs</output-generation-type>
    <variability-type>Fixed</variability-type>
    <number-of-outputs>6</number-of-outputs>
    <output-name>{LIST:CSV file for robot,Log file,Log2 file,CSV file for manual,Log3 file,Library Index Validation Log }</output-name>
  </process-output>
  <process-type-attribute name="ConsumeVolume">true</process-type-attribute>
  <process-type-attribute name="ContextCode">SAM</process-type-attribute>
  <process-type-attribute name="DefaultPlacementPattern">Place samples in same shape as input samples</process-type-attribute>
  <process-type-attribute name="Enabled">true</process-type-attribute>
  <process-type-attribute name="Family">Configured</process-type-attribute>
  <process-type-attribute name="IconConstant">LEAF</process-type-attribute>
  <process-type-attribute name="InheritsQC">false</process-type-attribute>
  <process-type-attribute name="ModifyInputOutput">All</process-type-attribute>
  <process-type-attribute name="OnlyEnableDoneAtLastPanel">false</process-type-attribute>
  <process-type-attribute name="OpenPostProcess">false</process-type-attribute>
  <process-type-attribute name="OutputContextCode">SA</process-type-attribute>
  <process-type-attribute name="OutputSorting">Input LIMS ID</process-type-attribute>
  <process-type-attribute name="ProcessGroup">Configured Processes</process-type-attribute>
  <process-type-attribute name="ProcessTabView">Use the default display</process-type-attribute>
  <process-type-attribute name="QCAdjustmentType">None</process-type-attribute>
